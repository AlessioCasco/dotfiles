[gist]
	private = yes

[include]
    path = ~/.gitconfig-local

[alias]
    a   = apply
    b   = branch
    cc  = checkout
    cb  = checkout -b
    cm  = checkout master
    cm  = commit -m
    d   = diff
    pl  = pull
    ph  = push
    sh  = stash
    st  = status

[color]
        diff = auto
        status = auto
        branch = auto
        ui = auto

[color "diff"]
        meta = yellow
        frag = white
        old = red
        new = green

[color "status"]
        added = green
        changed = yellow
        untracked = blue
